{
  "extends": "@plasmo/config/ts/nextjs.json",
  "include": [
    "next-env.d.ts",
    "index.d.ts",
    "./src/**/*.ts",
    "./src/**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "paths": {
      "~*": [
        "./src/*"
      ]
    },
    "baseUrl": ".",
    "plugins": [
      {
        "name": "next"
      }
    ]
  }
}
