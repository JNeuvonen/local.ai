{
  "extends": "@plasmo/config/ts/nextjs.json",
  "include": [
    "next-env.d.ts",
    "index.d.ts",
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "../../models/**/*.ts"
  ],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "paths": {
      "~*": ["./src/*"],
      "@models/*": ["../../models/*"]
    },
    "baseUrl": "."
  }
}
